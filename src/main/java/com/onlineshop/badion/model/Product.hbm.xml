<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.onlineshop.badion.model.Product" table="Product">
        <id column="id" name="id" type="java.lang.Integer">
        	<generator class="increment"></generator>
        </id>
        <property column="name" name="name" type="java.lang.String" />
        <property column="price" name="price" type="java.lang.Double" />
        <property column="description" name="description" type="java.lang.String" />
        <property column="id_group" name="idGroup" type="java.lang.Integer" />
        <property column="quantity" name="quantity" type="java.lang.Integer" />
       <!--   <set name="checkout" table="checkout_products" inverse="false" lazy="true" fetch="select">
        	<key>
        		<column name="id_product" not-null="true" />
        	</key>
        	<many-to-many entity-name="com.onlineshop.badion.model.Checkout">
        		<column name="id_order" not-null="true" />
        	</many-to-many>
        </set>  -->
    </class>
</hibernate-mapping>