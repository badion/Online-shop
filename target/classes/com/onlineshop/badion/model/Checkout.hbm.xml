<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.onlineshop.badion.model.Checkout" table="Checkout">
        <id column="id" name="id" type="java.lang.Integer">
        	<generator class="increment"></generator>
        </id>
        <property column="id_customer" name="idCustomer" type="java.lang.Integer" />
        <property column="order_status" name="orderStatus" type="java.lang.String" />
        <property column="date" name="date" type="java.util.Date" />
        <property column="price_sum" name="priceSum" type="java.lang.Double" />
