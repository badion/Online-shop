<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.onlineshop.badion.model.CheckoutProduct" table="Checkout_products">
        <id column="id" name="id" type="java.lang.Integer">
        	<generator class="increment"></generator>
        </id>
        <property column="id_product" name="idProduct" type="java.lang.Integer" />
        <property column="id_order" name="idCheckout" type="java.lang.Integer" />
    </class>
</hibernate-mapping>